<!DOCTYPE html>
<html>
  <head>
    <title>Itenarator</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-default navbar-fixed-top">
      <div class="container">
        <div class="logo">
          <h4><strong>jumzado</strong></h4>
        </div>

        <div class="menu">
          <ul class="list-inline">
            <li>Become a Partner</li>
            <li>Login</li>
            <li>Sign Up</li>
            <li>Help</li>
          </ul>
        </div>
      </div>


    <div class="jumbotron" style="height: 284px; padding-top: 136px;background-image: url(/assets/header.jpg); margin:0px;">
      <div class="container">
        <center>
          <form class="form-inline">
            <div class="form-group">
              <input type="email" class="form-control" id="exampleInputEmail2" placeholder="Cape Town, South Africa ">
            </div>

            <div class="form-group">
              <input type="email" class="form-control" id="exampleInputEmail2" placeholder="Start Date ">
            </div>

            <div class="form-group">
              <input type="email" class="form-control" id="exampleInputEmail2" placeholder="End Date ">
            </div>
            <button type="submit" class="btn btn-default">Go to Dashboard</button>
        </form>
</center>
      </div>
    </div>
    </nav>

    <center>
      <%= yield %>
    </center>


    <br><br>
    <footer class="footer">
      <div class="container">

        <ul class="list-inline" style="float:right;">
          <li>Terms and Privacy</li>
          <li>Site Map</li>
          <li>
            <i class="fa fa-facebook"></i>
          </li>
          <li>
            <i class="fa fa-twitter"></i>
          </li>
          <li>
            <i class="fa fa-instagram"></i>
          </li>
        </ul>
      </div>
    </footer>
  </body>
</html>
